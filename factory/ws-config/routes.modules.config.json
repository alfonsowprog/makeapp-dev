      "application/core/Layout/BackScripts/**/*.js",
      "application/core/Home/BackScripts/**/*.js"
    ],
    "FrontScripts": [
      "application/core/Globals/FrontScripts/**/*.js",
      "application/core/Layout/FrontScripts/**/*.js",
      "application/core/Home/FrontScripts/**/*.js"
